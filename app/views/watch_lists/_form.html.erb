<%= simple_form_for(@watch_list) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :companycik %>
	<% temp = current_user.id %>
	<%= f.hidden_field :user_id, :value => temp %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => 'button' %>
  </div>
<% end %>
